All Jobs for <strong><%= @company.name %></strong>

<ul>
<% @jobs.each do |job| %>
  <li class=job_<%= job.id %>>
    <%= link_to job.title, company_job_path(job.company, job) %> at <%= job.company.name %>
    <br>
    <%= link_to "Edit", company_job_path(job.company, job), method: "delete" %>
    <%= link_to "Delete", edit_company_job_path(job.company, job) %>
  </li>
  <br>
  <% end %>