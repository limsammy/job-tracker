All Jobs for <strong><%= @company.name %></strong>

<ul>
<% @jobs.each do |job| %>
  <li class=job_<%= job.id %>>
    <%= link_to job.title, company_jobs_path(job.company) %> at <%= job.company.name %>
    <%= link_to "Delete", company_job_path(job.company, job), method: "delete" %>
  </li>
  <% end %>